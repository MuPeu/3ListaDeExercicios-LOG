package exercicios;

import java.util.Scanner;

public class exB_tabuada {

	public static void main(String[] args) {
		int i;
		double num, result;
		
		Scanner teclado = new Scanner(System.in);
		
		System.out.print("Digite um NÃºmero: ");
		num = teclado.nextInt();
		
		System.out.print("\n\n");
		
		i = 0;
		
		do {
			result = num * i;
			
			
		}while(i == 10);

	}

}

package exercicios;

public class exF_divisivelPor4 {

	public static void main(String[] args) {
		int rest, i = 0;
		
		System.out.println("Estes são todos os valores numéricos inteiros ímpares situados na faixa de 0 a 20:");
		
		do {
			rest = i % 4;
			if (rest == 0) {
				System.out.println(i);
			}
			i++;

			
		}while(i <= 200);
		
	}

}
